{
  "name": "ella",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "seed": "prisma db push && pnpm exec tsc --target 'es6' --moduleResolution 'node' --outDir 'prisma/seed/build/' prisma/seed/index.ts && node prisma/seed/build/index.js"
  },
  "devDependencies": {
    "@iconify/svelte": "^3.0.1",
    "@lucia-auth/adapter-prisma": "^0.1.4",
    "@lucia-auth/oauth": "^0.2.5",
    "@lucia-auth/sveltekit": "^0.5.1",
    "@prisma/client": "^4.8.0",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "lucia-auth": "^0.3.4",
    "postcss": "^8.4.20",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "prisma": "^4.8.0",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.0"
  },
  "type": "module"
}
